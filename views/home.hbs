    
    <h1 class="title">My Recipes</h1>
    <div class="recipe-list">
      {{#each recipes}}
        <div class="recipe">
          <a href="/recipes/{{this._id}}">
            <img src="{{this.image}}" alt="{{this.title}}">
            <h2>{{this.title}}</h2>
           
          </a>
        </div>
      {{/each}}
    </div>

